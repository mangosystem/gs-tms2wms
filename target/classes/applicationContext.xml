<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<!-- DXF Output Format -->
	<bean id="tmsConfigPanel"
		class="org.geoserver.web.data.resource.DataStorePanelInfo">
		<property name="id" value="tmsConfigPanel" />
		<property name="factoryClass" value="com.mango.tms.TMSFormat" />
		<property name="iconBase"
			value="org.geoserver.web.GeoServerApplication" />
		<property name="icon"
			value="img/icons/geosilk/page_white_raster.png" />
		<property name="componentClass"
			value="org.geoserver.web.data.store.raster.TMSRasterEditPanel" />
	</bean>

</beans>
